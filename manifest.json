{
  "name": "微博超话签到插件",
  "description" : "准时准点，抢微博超话签到第一",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
      "default_icon": "icon.png"
  },
  "content_scripts": [  
  {  
    "matches": ["*://www.weibo.com/p/*"],  
    "js": ["check-in.js"],
    "all_frames": true,
    "run_at": "document_idle"
  }  
] 
}

